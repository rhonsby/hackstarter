<div class='discover-container'>
  <div class='discover-index'>
    <div class='discover-heading center'>
      <h3>Discover <span class='lead'>Companies</span></h3>
      <h3 class='gray lead'>Passion, ideas, and ambition abound. Start exploring!</h3>
    </div>

    <div class='discover-content'>
      <div class='row company-form'>
        <div class='col-md-10 main-content'>
          <%= JST['companies/discover/discover_panels']({ companies: companies }) %>
        </div>

        <div class='col-md-2 main-sidebar'>
          <%= JST['companies/discover/sidebar']() %>
        </div>
      </div>
    </div>
  </div>
</div>